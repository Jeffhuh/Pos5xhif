@page "/stores"
<h3>Stores</h3>
<h4>Neuer Store</h4>
<div>
    <AddStore />
</div>
<h4>Alle Stores</h4>
<table class="table table-sm">
    <thead>
        <tr>
            <th>GUID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in StoreList)
        {
            <tr>
                <td>@s.Guid</td>
                <td>@s.Name</td>
            </tr>
        }
    </tbody>
</table>